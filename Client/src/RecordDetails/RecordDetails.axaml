<UserControl xmlns="https://github.com/avaloniaui"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
		x:Class="RiskiInsurance.RecordDetails"
		>
	<Panel>
		<Rectangle Fill="#000000" />
		<Border>
			<Grid RowDefinitions="Auto,Auto,Auto,Auto,Auto" ColumnDefinitions="*, Auto">
				<Button Grid.Column="1" Content="{Binding	ExitButtonText}"/>
				<Label Content="Test" Grid.Row="1"/>
				<Label Content="Test" Grid.Row="2"/>
				<Label Content="Test" Grid.Row="3"/>
				<Label Content="Test" Grid.Row="4"/>
			</Grid>
		</Border>
	</Panel>
	<UserControl.Styles>
		<Style Selector="Label">
			<Setter Property="Foreground" Value="White">
			</Setter>
		</Style>
	</UserControl.Styles>
</UserControl>
